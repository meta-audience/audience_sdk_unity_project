{
    "FLUSH_TIMER": {
        "duration": 5
    },
    "LOGGERS": {
        "default": {
            "level": "info",
            "sinks": [
                "stdout_color_sink",
                "basic_file_sink"
            ],
            "type": "sync"
        },
        "stats": {
            "level": "info",
            "sinks": [
                "stdout_color_sink",
                "stats_file_sink"
            ],
            "type": "sync"
        },
        "webrtc": {
            "level": "info",
            "sinks": [
                "stdout_color_sink",
                "webrtc_file_sink"
            ],
            "type": "sync"
        }
    },
    "PATTERNS": {
        "console_pattern": "[%C-%m-%d %H:%M:%S.%e]%^[%L]%$ %v",
        "general_pattern": "[%C-%m-%d %H:%M:%S.%e]%^[%L][%n]%$ %v"
    },
    "SINKS": {
        "basic_file_sink": {
            "filename": "D:\\BitBucket\\audience-sdk-unity-project\\audience_sdk_unity_project\\Assets\\audience-unity-sdk\\Plugins\\logs\\audience_logfile.log",
            "level": "info",
            "pattern": "general_pattern",
            "truncate": true,
            "type": "basic_file_sink_mt"
        },
        "stats_file_sink": {
            "filename": "D:\\BitBucket\\audience-sdk-unity-project\\audience_sdk_unity_project\\Assets\\audience-unity-sdk\\Plugins\\logs\\audience_stats_logfile.log",
            "level": "info",
            "pattern": "general_pattern",
            "truncate": true,
            "type": "basic_file_sink_mt"
        },
        "stdout_color_sink": {
            "level": "critical",
            "pattern": "console_pattern",
            "type": "stdout_color_sink_mt"
        },
        "webrtc_file_sink": {
            "filename": "D:\\BitBucket\\audience-sdk-unity-project\\audience_sdk_unity_project\\Assets\\audience-unity-sdk\\Plugins\\logs\\audience_webrtc_logfile.log",
            "level": "info",
            "pattern": "general_pattern",
            "truncate": true,
            "type": "basic_file_sink_mt"
        }
    },
    "THREAD_POOL": {
        "queue_size": 8192,
        "thread_count": 2
    }
}
